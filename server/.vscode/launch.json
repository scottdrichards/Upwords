    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Server",
            "skipFiles": [
                "${workspaceRoot}/node_modules/**",
                "<node_internals>/**"
            ],
            "sourceMaps": true,
            "program": "${workspaceFolder}/dist/server/src/server.js",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": ["${workspaceFolder}/dist/**/*.js"]
        }
    ]
}